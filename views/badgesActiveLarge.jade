include mixins
extends layout
block content   

            include navbar

            
            h3 List of active badges
            h5 Use Ctrl-F to search list
            br

            div
                table#pickle.table.table-hover.table-striped.table-bordered
                    thead
                        tr.warning
                            th Badge number
                            th Status
                            th Employee Id
                    tbody
                        -each result in results
                            tr
                                    td=result.iClassNumber
                                    td=result.StatusName
                                    td=result.EmpID
    
 




    
